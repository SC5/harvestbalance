{
  "name": "Harvest hour balance",
  "description": "Shows the current hour balance for logged in user",
  "version": "2.0",
  "manifest_version": 2,
  "update_url": "http://www.google.com/",
  "permissions": [ "tts" ],
  "content_scripts": [
    {
      "matches": ["https://*.harvestapp.com/*"],
      "css": ["styles.css"],
      "js": ["harvest.js"]
    }
  ],
  "web_accessible_resources": [
    "main-panel.html",
    "settings-panel.html"
  ]
}
